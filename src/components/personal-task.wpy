        padding: 20rpx 20rpx;
        text-align: left;
        box-sizing: border-box;
        border-radius: 20rpx;
        position: relative;
        overflow: hidden;
        background-color: #fff;
    }
    .task-tag {
        position: relative;
        text-align: center;
        height: 60rpx;
    }
    .task-tag .dashed-line {
        position: absolute;
        left: 0;
        top: 50%;
        width: 100%;
        height: 1px;
        border-top: 1px dashed #ccc;
    }
    .task-tag .tag {
        background: #fff;
        padding: 5rpx 40rpx;
        margin: 0 auto;
        z-index: 10;
        position: absolute;
        left: 50%;
        top: 0;
        height: 60rpx;
        line-height: 60rpx;
        transform:translate3d(-50%,0,0);
    }
    .personal-task .row-block {
        padding: 14rpx 0;
    }
    .personal-task .task-content .task-title-block{
        color: #02CDF9;
        font-size: 36rpx;
    }
    .personal-task .task-content .task-title-block .normal {
        color: #666;
        font-size: 32rpx;
    }
    .personal-task.female .task-content .task-title-block {
        color: #ff7d7d;
    }
    .personal-task .task-content .flex-box {
        width: 80%;
    }
    .star-count {
        height: 60rpx;
        line-height: 60rpx;
        width: 60%;
        display: flex;
        justify-content: flex-start;
        margin-top: 30rpx;
    }
    .star-count .icon {
        display: inline-block;
        width: 40rpx;
        height: 40rpx;
        vertical-align: middle;
    }
    .star-count .block {
        width: 33.3%;
        line-height: 1;
    }
    .star-count .block text {
        vertical-align:middle;
        display:inline-block;
        margin-left:10rpx;
    }
    .star-count .block text.faved {
        color: #1abcb0;
    }
    .star-count .block.star-block {
        color: #f80;
    }
    .button-block .running, .button-block .paused {
        padding: 30rpx 0;
    }
    .personal-task-container {
        z-index: 10;
    }
    .status-mark {
        position: absolute;
        width: 200rpx;
        height: 160rpx;
        right: 30rpx;
        top: 60rpx;
        z-index: 2;
    }
    .personal-task.stopped {
        background-image: url(data:image/png;base64,);
        background-size: 200rpx 160rpx;
        background-position: calc(100vw - 250rpx) 120rpx;
        background-repeat: no-repeat;
    }
    .personal-task.paused {
        background-image: url(data:image/png;base64,);
        background-size: 200rpx 160rpx;
        background-position: calc(100vw - 250rpx) 120rpx;
        background-repeat: no-repeat;
    }
</style>
